{
  "name": "uikit-react",
  "version": "3.0.18",
  "description": "UIkit components for use in React.",
  "repository": "https://github.com/vacarsu/uikit-react",
  "homepage": "https://vacarsu.github.io/uikit-react",
  "main": "./lib/uikit-react.js",
  "module": "./lib/uikit-react.js",
  "scripts": {
    "build": "rollup -c",
    "test": "jest --no-cache",
    "build-storybook": "build-storybook",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "lint": "tslint -c tslint.json \"src/**/*.ts\"",
    "lint:fix": "tslint --fix -c tslint.json \"src/**/*.ts\"",
    "precommit": "lint-staged"
  },
  "author": "Alex Lopez",
  "license": "ISC",
  "peerDependencies": {
    "classnames": "^2.2.5",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "uikit": "^3.0.0-rc.5"
  },
  "devDependencies": {
    "@storybook/addon-info": "^3.4.8",
    "@storybook/addon-knobs": "^3.4.8",
    "@storybook/addon-options": "^3.4.8",
    "@storybook/react": "4.0.0-alpha.3",
    "@types/classnames": "^2.2.4",
    "@types/enzyme": "^3.1.12",
    "@types/he": "^1.1.0",
    "@types/marked": "^0.3.0",
    "@types/prismjs": "^1.9.0",
    "@types/react": "^16.3.16",
    "@types/react-dom": "^16.0.5",
    "@types/react-router": "^4.0.26",
    "@types/react-router-dom": "^4.2.7",
    "@types/uikit": "^2.27.6",
    "awesome-typescript-loader": "^5.2.0",
    "enzyme": "^3.3.0",
    "he": "^1.1.1",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "lint-staged": "^7.2.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.13.7",
    "react": "^16.4.1",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "react-dom": "^16.4.1",
    "react-jsx-parser": "^1.3.4",
    "react-router-dom": "^4.3.1",
    "react-test-renderer": "^16.4.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.60.7",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-css-only": "^0.4.0",
    "rollup-plugin-node-globals": "^1.2.1",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-serve": "^0.4.2",
    "rollup-plugin-typescript2": "^0.14.0",
    "storybook-addon-jsx": "^5.3.0",
    "ts-loader": "^4.3.1",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.9.2",
    "typescript": "^2.9.1",
    "uglify-es": "^3.3.9",
    "uikit": "^3.0.0-rc.9"
  },
  "dependencies": {
    "@types/jest": "^23.3.0",
    "jest": "^23.4.1",
    "ts-jest": "^23.0.1"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,tsx}"
    ],

    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lib/"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
